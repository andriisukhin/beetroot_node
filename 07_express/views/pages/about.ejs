<%- contentFor('hero') %>
<div class="container">
    <h1>About page</h1>
</div>

<%- contentFor('body') %>
<main>
    <h2>User info</h2>
    <div>
        <% for(user of users) { %>
            <div>
                <h3><%= user.name %></h3>
                <img src="<%= user.avatar %>" alt="<%= user.name %>">
            </div>
        <% } %>
    </div>
</main>